    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasAudience -->

    <ObjectProperty rdf:about="&cao;hasAudience">
        <rdfs:label>audience</rdfs:label>
        <rdfs:range rdf:resource="&cao;Audience"/>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:subPropertyOf rdf:resource="&skos;related"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasContact -->

    <ObjectProperty rdf:about="&cao;hasContact">
        <rdfs:label>contact</rdfs:label>
        <rdfs:range rdf:resource="&cao;Contact"/>
