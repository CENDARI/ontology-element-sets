<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY prov "http://www.w3.org/ns/prov#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY vcard "http://www.w3.org/2006/vcard/ns#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY cao "http://resources.cendari.dariah.eu/ontologies/cao/" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY iso639-1 "http://id.loc.gov/ontologies/iso639-1_Languages" >
]>


<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	 xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:cao="http://resources.cendari.dariah.eu/ontologies/cao/"
     xmlns:iso639-1="http://id.loc.gov/ontologies/iso639-1_Languages"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vcard="http://www.w3.org/2006/vcard/ns#">
    <Ontology rdf:about="http://resources.cendari.dariah.eu/ontologies/cao">
        <imports rdf:resource="http://id.loc.gov/ontologies/iso639-1_Languages"/>
        <imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <imports rdf:resource="http://www.w3.org/2006/vcard/ns"/>
        <imports rdf:resource="http://www.w3.org/ns/org#"/>
        <imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
    </Ontology>
    


