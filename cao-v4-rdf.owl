        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;InstitutionIdentity"/>
        <rdfs:subPropertyOf rdf:resource="&cao;hasIdentity"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasPhone -->

    <ObjectProperty rdf:about="&cao;hasPhone">
        <rdfs:subPropertyOf rdf:resource="&vcard;hasTelephone"/>
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cao;Contact"/>
                    <rdf:Description rdf:about="&cao;RepositoryAddress"/>
                </unionOf>
            </Class>
        </rdfs:domain>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasRecordId -->

    <ObjectProperty rdf:about="&cao;hasRecordId">
        <rdfs:label>id</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:subPropertyOf rdf:resource="&vcard;hasUID"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasRepository -->

    <ObjectProperty rdf:about="&cao;hasRepository">
        <rdfs:label>repository</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;Repository"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/org#hasSite"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasRepositoryAddress -->

    <ObjectProperty rdf:about="&cao;hasRepositoryAddress">
        <rdfs:label>address</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Repository"/>
        <rdfs:range rdf:resource="&cao;RepositoryAddress"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/org#siteAddress"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasRepositoryTypes -->

    <ObjectProperty rdf:about="&cao;hasRepositoryTypes">
        <rdfs:label>types</rdfs:label>
        <rdfs:domain rdf:resource="&cao;CendariRepositoryTypeScheme"/>
        <rdfs:range rdf:resource="&cao;RepositoryType"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasService -->

    <ObjectProperty rdf:about="&cao;hasService">
        <rdfs:label>service</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Repository"/>
        <rdfs:range rdf:resource="&cao;RepositoryServices"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasSources -->

    <ObjectProperty rdf:about="&cao;hasSources">
        <rdfs:label>sources</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;SourceInformation"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasTopics -->

    <ObjectProperty rdf:about="&cao;hasTopics">
        <rdfs:label>topics</rdfs:label>
        <rdfs:domain rdf:resource="&cao;CendariTopicScheme"/>
        <rdfs:range rdf:resource="&cao;Topic"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/isPerson -->

    <ObjectProperty rdf:about="&cao;isPerson">
        <rdfs:label>isPerson</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Contact"/>
        <rdfs:subPropertyOf rdf:resource="&vcard;hasRelated"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/isRepositoryOfType -->

    <ObjectProperty rdf:about="&cao;isRepositoryOfType">
        <rdfs:label>type</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Repository"/>
        <rdfs:range rdf:resource="&cao;RepositoryType"/>
        <rdfs:subPropertyOf rdf:resource="&skos;related"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/usesConventionsOrStandards -->

    <ObjectProperty rdf:about="&cao;usesConventionsOrStandards">
        <rdfs:label>conventions</rdfs:label>
        <rdfs:range rdf:resource="&cao;ConventionOrStandard"/>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:subPropertyOf rdf:resource="&skos;related"/>
    </ObjectProperty>
    


    <!-- http://www.w3.org/2006/vcard/ns#hasEmail -->

    <rdf:Description rdf:about="&vcard;hasEmail">
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cao;Contact"/>
                    <rdf:Description rdf:about="&cao;RepositoryAddress"/>
                </unionOf>
            </Class>
        </rdfs:domain>
    </rdf:Description>
    


    <!-- http://www.w3.org/2006/vcard/ns#hasGeo -->

    <rdf:Description rdf:about="&vcard;hasGeo">
        <rdfs:domain rdf:resource="&cao;RepositoryAddress"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/2006/vcard/ns#hasLogo -->

    <rdf:Description rdf:about="&vcard;hasLogo">
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cao;Institution"/>
                    <rdf:Description rdf:about="&cao;Repository"/>
                </unionOf>
            </Class>
        </rdfs:domain>
    </rdf:Description>
    


    <!-- http://www.w3.org/2006/vcard/ns#hasPhoto -->

    <rdf:Description rdf:about="&vcard;hasPhoto">
        <rdfs:domain rdf:resource="&cao;Repository"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/2006/vcard/ns#hasURL -->

    <rdf:Description rdf:about="&vcard;hasURL">
        <rdfs:domain rdf:resource="&cao;RepositoryAddress"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/Note -->

    <DatatypeProperty rdf:about="&cao;Note">
        <rdfs:label>Note</rdfs:label>
        <rdfs:comment>Data property representing various notes associated to the institution and repository descriptions e.g. cao:holdingsNote, cao:repositoryHistoryNote, cao:sourceNote etc.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </DatatypeProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/URI -->

    <DatatypeProperty rdf:about="&cao;URI">
        <rdfs:label>URI</rdfs:label>
        <rdfs:comment>A URI to the resource from where repository history or holdings descriptions are created.

Corresponds to /control/sources/source@href and /control/sources/source@id


In case there is not online representation of the resource, URI has to be provided as well and manually defined by the user. 

For example,  in this case it may be of a format such as :

&lt;scheme&gt;:&lt;some identifying string&gt;

where the scheme is practically a (cendari internal) URI scheme such as:

caoid  (Cendari Archival Ontology Identifiers)





</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;SourceInformation"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </DatatypeProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/addressType -->

    <DatatypeProperty rdf:about="&cao;addressType">
        <rdfs:label>address type</rdfs:label>
        <rdfs:comment>Type of address recorded as a repository address. Permits to specify what type of address is recorded (postal, visitors, etc.). 

Corresponds to /desc/repositories/repository/location@type attribute 
</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;RepositoryAddress"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:subPropertyOf rdf:resource="&vcard;note"/>
    </DatatypeProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasMaintenanceStatus -->

    <DatatypeProperty rdf:about="&cao;hasMaintenanceStatus">
        <rdfs:label>Status</rdfs:label>
        <rdfs:comment>corresponds to /eag/control/maintenancestatus

Maintenance status of the institutional description. Possible values:
new, cancelled, deleted, deletedReplaced, deletedMerged, deletedSplit, derived or revised</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:range>
            <rdfs:Datatype>
                <oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>cancelled</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>deleted</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="&rdf;List"/>
                                        <rdf:first>deletedMerged</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="&rdf;List"/>
                                                <rdf:first>deletedReplaced</rdf:first>
                                                <rdf:rest>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                        <rdf:first>deletedSplit</rdf:first>
                                                        <rdf:rest>
                                                            <rdf:Description>
                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                <rdf:first>derived</rdf:first>
                                                                <rdf:rest>
                                                                    <rdf:Description>
                                                                        <rdf:type rdf:resource="&rdf;List"/>
                                                                        <rdf:first>new</rdf:first>
                                                                        <rdf:rest>
                                                                            <rdf:Description>
                                                                                <rdf:type rdf:resource="&rdf;List"/>
                                                                                <rdf:first>revised</rdf:first>
                                                                                <rdf:rest rdf:resource="&rdf;nil"/>
                                                                            </rdf:Description>
                                                                        </rdf:rest>
                                                                    </rdf:Description>
                                                                </rdf:rest>
                                                            </rdf:Description>
                                                        </rdf:rest>
                                                    </rdf:Description>
                                                </rdf:rest>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </DatatypeProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasScript -->

    <DatatypeProperty rdf:about="&cao;hasScript">
        <rdfs:label>Script</rdfs:label>
        <rdfs:comment>Scripts in Use
TODO: make separate class and generate vocabulary  from ISO 15924 List (create concept scheme for scripts in  ISO 15924 )</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;LanguageDeclaration"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:range>
            <rdfs:Datatype>
                <oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>Cyrilic</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>Latin</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </DatatypeProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/holdingsNote -->

    <DatatypeProperty rdf:about="&cao;holdingsNote">
        <rdfs:label>Holdings</rdfs:label>
        <rdfs:comment>Based on specified source (with URL, SourceNote) 
corresponding to /holdings/descriptiveNote element in CENDARI@EAG</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&cao;Note"/>
        <rdfs:domain rdf:resource="&cao;SourceInformation"/>
        <propertyDisjointWith rdf:resource="&cao;repositoryHistoryNote"/>
        <propertyDisjointWith rdf:resource="&cao;sourceNote"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </DatatypeProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/name -->

    <DatatypeProperty rdf:about="&cao;name">
        <rdfs:domain rdf:resource="&cao;InstitutionIdentity"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
