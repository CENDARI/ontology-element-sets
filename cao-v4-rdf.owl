    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasMaintenanceAgency -->

    <ObjectProperty rdf:about="&cao;hasMaintenanceAgency">
        <rdfs:label>agency</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;MaintenanceAgency"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasMaintenanceEvent -->

    <ObjectProperty rdf:about="&cao;hasMaintenanceEvent">
        <rdfs:label>event</rdfs:label>
        <rdfs:comment>property relating the maintenance history of the institution with the maintenance activity (prov:Activity)
</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;MaintenanceHistory"/>
        <rdfs:range rdf:resource="&prov;Activity"/>
        <rdfs:subPropertyOf rdf:resource="&prov;activity"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasMaintenanceHistory -->

    <ObjectProperty rdf:about="&cao;hasMaintenanceHistory">
        <rdfs:label>history</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;MaintenanceHistory"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasNonPreferredName -->

    <ObjectProperty rdf:about="&cao;hasNonPreferredName">
        <rdfs:label>non preferred name</rdfs:label>
        <rdfs:comment>see cao:InstitutionIdentity</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;InstitutionIdentity"/>
        <rdfs:subPropertyOf rdf:resource="&cao;hasIdentity"/>
        <propertyDisjointWith rdf:resource="&cao;hasParallelName"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasParallelName -->

    <ObjectProperty rdf:about="&cao;hasParallelName">
