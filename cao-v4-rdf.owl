    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/authForm -->

    <AnnotationProperty rdf:about="&cao;authForm">
        <rdfs:domain rdf:resource="&cao;InstitutionIdentity"/>
