                </unionOf>
            </Class>
        </rdfs:domain>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasIdentity -->

    <ObjectProperty rdf:about="&cao;hasIdentity">
        <rdfs:label>identity</rdfs:label>
        <rdfs:comment>see cao:InstitutionIdentity</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;Institution"/>
        <rdfs:range rdf:resource="&cao;InstitutionIdentity"/>
        <propertyDisjointWith rdf:resource="&cao;hasNonPreferredName"/>
        <propertyDisjointWith rdf:resource="&cao;hasParallelName"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasLanguage -->

    <ObjectProperty rdf:about="&cao;hasLanguage">
        <rdfs:label>language</rdfs:label>
        <rdfs:comment>Ranges to values from ISO-639-1 language vocabulaty.</rdfs:comment>
        <rdfs:domain rdf:resource="&cao;LanguageDeclaration"/>
        <rdfs:range rdf:resource="http://id.loc.gov/vocabulary/iso639-1/iso639-1_Language"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasLanguageDeclaration -->

    <ObjectProperty rdf:about="&cao;hasLanguageDeclaration">
        <rdfs:label>declaration</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Institution"/>
