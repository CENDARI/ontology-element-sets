        <rdfs:range rdf:resource="http://www.w3.org/ns/org#FormalOrganization"/>
        <rdfs:domain>
            <Class>
                <intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cao;Contributor"/>
                    <rdf:Description rdf:about="&foaf;Person"/>
                </intersectionOf>
            </Class>
        </rdfs:domain>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasAgencyCode -->

    <ObjectProperty rdf:about="&cao;hasAgencyCode">
        <rdfs:label>agency code</rdfs:label>
        <rdfs:domain rdf:resource="&cao;MaintenanceAgency"/>
        <rdfs:subPropertyOf rdf:resource="&vcard;hasUID"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasAgencyName -->

    <ObjectProperty rdf:about="&cao;hasAgencyName">
        <rdfs:label>name</rdfs:label>
        <rdfs:domain rdf:resource="&cao;MaintenanceAgency"/>
        <rdfs:subPropertyOf rdf:resource="&vcard;hasName"/>
    </ObjectProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasAgent -->

    <ObjectProperty rdf:about="&cao;hasAgent">
        <rdfs:label>agent</rdfs:label>
        <rdfs:range rdf:resource="&cao;Contributor"/>
