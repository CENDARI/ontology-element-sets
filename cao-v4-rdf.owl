    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasHoldingsInTopic -->

    <ObjectProperty rdf:about="&cao;hasHoldingsInTopic">
        <rdfs:label>holdings</rdfs:label>
        <rdfs:range rdf:resource="&cao;Topic"/>
        <rdfs:subPropertyOf rdf:resource="&skos;related"/>
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&cao;Institution"/>
