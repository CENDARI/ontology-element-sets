    </AnnotationProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasConventions -->

    <AnnotationProperty rdf:about="&cao;hasConventions">
        <rdfs:label>convention</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&skos;hasTopConcept"/>
    </AnnotationProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasRepositoryTypes -->

    <AnnotationProperty rdf:about="&cao;hasRepositoryTypes">
        <rdfs:label>types</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&skos;hasTopConcept"/>
    </AnnotationProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasTopics -->

    <AnnotationProperty rdf:about="&cao;hasTopics">
        <rdfs:label>topics</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&skos;hasTopConcept"/>
    </AnnotationProperty>
    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/parForm -->

    <AnnotationProperty rdf:about="&cao;parForm">
        <rdfs:domain rdf:resource="&cao;InstitutionIdentity"/>
        <rdfs:subPropertyOf rdf:resource="&skos;altLabel"/>
    </AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://resources.cendari.dariah.eu/ontologies/cao/hasAccessNotes -->

    <ObjectProperty rdf:about="&cao;hasAccessNotes">
        <rdfs:label>access notes</rdfs:label>
        <rdfs:domain rdf:resource="&cao;Repository"/>
        <rdfs:range rdf:resource="&cao;RepositoryAccess"/>
        <rdfs:subPropertyOf rdf:resource="&vcard;hasRelated"/>
    </ObjectProperty>
    


